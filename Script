local Players = game:GetService("Players")
for _, player in pairs(Players:GetPlayers()) do
    if player.Name ~= LocalPlayer.Name then
        local playerfolder = Instance.new("Folder",workspace)
        playerfolder.Name = "folder"..player.Name
        local mousetracking = Instance.new("Part",playerfolder)
        mousetracking.Material = Enum.Material.Neon
        mousetracking.Transparency = 1
        mousetracking.Shape = Enum.PartType.Ball
        mousetracking.Anchored = true
        mousetracking.CanCollide = false
        coroutine.wrap(function()
            game:GetService("RunService").Stepped:Connect(function()
                if player then
                    local playerpos = kit:GetServerMousePos(player)
                    mousetracking.Position = playerpos
                end
            end)
        end)()
    end
end

Players.PlayerAdded:Connect(function(player)
	local playerfolder = Instance.new("Folder",workspace)
	playerfolder.Name = "folder"..player.Name
    local mousetracking = Instance.new("Part",playerfolder)
    mousetracking.Material = Enum.Material.Neon
    mousetracking.Anchored = true
    mousetracking.Transparency = 1
    mousetracking.CanCollide = false
    mousetracking.Shape = Enum.PartType.Ball
    coroutine.wrap(function()
		game:GetService("RunService").Stepped:Connect(function()
            if player then
                local playerpos = kit:GetServerMousePos(player)
                mousetracking.Position = playerpos
            end
		end)
	end)()
end)

Players.PlayerRemoving:Connect(function(player)
    workspace:FindFirstChild("folder"..player.Name):Destroy()
end)

LocalPlayer.CharacterAdded:Connect(function(character)
    task.wait(1)
    print("Player Spawned")
    character.Humanoid.Died:Connect(function()
            if getgenv().AutoRefresh == true then
                print("Verified Death")
                local plr = LocalPlayer
                local chr = plr.Character
                local cpos = chr.PrimaryPart.CFrame
                local Xtable = chr:GetChildren()
                for i,v in pairs(Xtable) do
                    if v:IsA("BasePart" or "Part") then
                        v:Remove()
                    end
                end
                local newChar = Instance.new("Model")
                newChar.Parent = workspace
                plr.Character = newChar
                task.wait()
                plr.Character = chr
                newChar:Destroy()
                game.Players.LocalPlayer.CharacterAdded:Wait():WaitForChild("HumanoidRootPart")
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = cpos
            end
    end)
end)

local GetPlayer = function(string)
    local lower = string:lower()
    for _, plr in next, game.Players:GetPlayers() do
        if plr.Name:sub(1,#string):lower() == lower or plr.DisplayName:sub(1,#string):lower() == lower then
            return plr
        end
    end
	return nil
end

local function RefreshCharacter()
    local plr = LocalPlayer
    local chr = Character
    local cpos = chr.PrimaryPart.CFrame
    local Xtable = chr:GetChildren()
    for i,v in pairs(Xtable) do
        if v:IsA("BasePart" or "Part") then
            v:Remove()
        end
    end
    local newChar = Instance.new("Model")
    newChar.Parent = workspace
    plr.Character = newChar
    task.wait()
    plr.Character = chr
    newChar:Destroy()
    game.Players.LocalPlayer.CharacterAdded:Wait():WaitForChild("HumanoidRootPart")
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = cpos
end

local function GetNearestPlayer()
    local leastdist = 50
    local p = nil

    for i,v in pairs(game.Players:GetPlayers()) do
        if v == game.Players.LocalPlayer then continue end
        if v.Character == nil or v.Character.Parent == nil or v.Character:FindFirstChild('HumanoidRootPart') == nil then continue end
        if v.Character.Humanoid.Health <= 0 then continue end
        local d = game.Players.LocalPlayer:DistanceFromCharacter(v.Character.HumanoidRootPart.Position)
        if d < leastdist then
            leastdist = d
            p = v
        end
    end

    return p
end

UserInputService.InputBegan:Connect(function(inp,gp)
    if gp then return end
    if inp.KeyCode == Enum.KeyCode.Q then
        if getgenv().ModdedPunch == true then
            local target = GetNearestPlayer()
            local targetchar = target.Character
            Punch:FireServer(targetchar)
            print("Fired punch remote on closest player ("..target..").")
        end
    end
end)


