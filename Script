local sysmsg = function(msg,font)
    if font then
        game.StarterGui:SetCore( "ChatMakeSystemMessage",  { Text = msg, Color = Color3.fromRGB(255,255,255), Font = Enum.Font[font], FontSize = Enum.FontSize.Size18 } )
    
    elseif not font then
        game.StarterGui:SetCore( "ChatMakeSystemMessage",  { Text = msg, Color = Color3.fromRGB(255,255,255), Font = Enum.Font.Gotham, FontSize = Enum.FontSize.Size18 } )
    else
        warn("Invalid arguments")
    end
end       
local syserror = function(msg,font)
    if font then
        game.StarterGui:SetCore( "ChatMakeSystemMessage",  { Text = msg, Color = Color3.fromRGB(255, 0, 0), Font = Enum.Font[font], FontSize = Enum.FontSize.Size18 } )
    
    elseif not font then
        game.StarterGui:SetCore( "ChatMakeSystemMessage",  { Text = msg, Color = Color3.fromRGB(255, 0, 0), Font = Enum.Font.Gotham, FontSize = Enum.FontSize.Size18 } )
    else
        warn("Invalid arguments")
    end
end

local function write(obj,txt)
	for T = 1, #txt do
		obj.Text = string.sub(txt, 1, T)
		task.wait(0.02)
	end		
end


