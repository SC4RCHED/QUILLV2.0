local Notify = function(msg,title)
    local backround = BackroundNotification:Clone()
    backround.ClipsDescendants = true
    backround.Size = UDim2.new(0, 292,0, 0)
    backround.BackgroundTransparency = 1
    local Stroke = Instance.new("UIStroke",backround)
    Stroke.Color = Color3.new(255,255,255)
    Stroke.Transparency = 1
    backround.Parent = ScreenGui.Frame
    local titleobject = backround.Title
    local backroundobject = backround.Text
    local openingtween = TweenService:Create(backround,TweenInfo.new(.3,Enum.EasingStyle.Quad),{Size = UDim2.new(0, 292,0, 68),BackgroundTransparency = 0}):Play()
    local stroketween1 = TweenService:Create(Stroke,TweenInfo.new(.2,Enum.EasingStyle.Quad),{Transparency = 0}):Play()
    task.wait(.2)
    write(titleobject,title)
    write(backroundobject,msg)
    task.wait(3)

    local closingtween = TweenService:Create(backround,TweenInfo.new(.7,Enum.EasingStyle.Quad),{Size = UDim2.new(0, .5,0, 68),BackgroundTransparency = 1}):Play()
    local stroketween2 = TweenService:Create(Stroke,TweenInfo.new(.2,Enum.EasingStyle.Quad),{Transparency = 1}):Play()
    task.wait(.9)
    backround:Destroy()
end

