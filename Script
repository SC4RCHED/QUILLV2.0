LocalPlayer.Chatted:Connect(function(message)
    message = string.lower(message)

    local splitString = message:split(" ")

    local slashCommand = splitString[1]

    local cmd = slashCommand:split(Prefix)

    local cmdsName = cmd[2]

    if CMDS[cmdsName] then

        local arguments = {}

        for i = 2, #splitString, 1 do
            table.insert(arguments,splitString[i])
        end

        CMDS[cmdsName](LP,arguments)

    end
end)

CMDS.autore = function(sender,args)
    if VIPP == true then

        local toggle = args[1]
        if toggle == "on" then
            getgenv().AutoRefresh = true
            sysmsg("Auto Refresh on.")
            Character.Humanoid.Died:Connect(function()
                if getgenv().AutoRefresh == true then
                    print("Verified Death")
                    local plr = game.Players.LocalPlayer
                    local chr = plr.Character
                    local cpos = chr.PrimaryPart.CFrame
                    local Xtable = chr:GetChildren()
                    for i,v in pairs(Xtable) do
                        if v:IsA("BasePart" or "Part") then
                            v:Remove()
                        end
                    end
                    local newChar = Instance.new("Model")
                    newChar.Parent = workspace
                    plr.Character = newChar
                    task.wait()
                    plr.Character = chr
                    newChar:Destroy()
                    game.Players.LocalPlayer.CharacterAdded:Wait():WaitForChild("HumanoidRootPart")
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = cpos
        end
    end)
end
    elseif toggle == "off" then
        getgenv().AutoRefresh = false
        sysmsg("Auto Refresh off.")
    else
        syserror("Please enter either 'On' or 'Off'.")
    end
end

CMDS.notif = function(sender,args)
    if ADMINP == true then
        if args[1] and args[2] then
            local M = args[1]
            local T = args[2]
            Notify(M,T)
        end
    end
end

CMDS.modpunch = function(sender,args)
    if MemberP == true then
        local toggle = args[1]
        if toggle == "on" then
            getgenv().ModdedPunch = true
            sysmsg("Modded Punch on.")
        elseif toggle == "off" then
            sysmsg("Modded Punch off.")
            getgenv().ModdedPunch = false
        else
            syserror("Please enter either 'On' or 'Off'.")
        
        end
    end
end

CMDS.fps = function(sender,args)
    local limit = args[1]
    if type(limit) == 'number' then
        if limit % 1 == 0 then
            --setfpscap(limit)
            syserror("Your executor does not support "..limit.." FPS.")
        else
            syserror("Please provide an integer (Full number).")
        end
    else
        syserror("Invalid arguments. Please provide argument (NUMBER).")
    end
end

CMDS.refresh = function(sender,args)
    if MemberP == true then
        local plr = game.Players.LocalPlayer
        local chr = plr.Character
        local cpos = chr.PrimaryPart.CFrame
        local Xtable = chr:GetChildren()
        for i,v in pairs(Xtable) do
            if v:IsA("BasePart" or "Part") then
                v:Remove()
            end
        end
        local newChar = Instance.new("Model")
        newChar.Parent = workspace
        plr.Character = newChar
        task.wait()
        plr.Character = chr
        newChar:Destroy()
        game.Players.LocalPlayer.CharacterAdded:Wait():WaitForChild("HumanoidRootPart")
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = cpos
    end
end


CMDS.removedespawn = function(sender,args)
    local a = string.lower(args[1])
    if a == "on" then
        sysmsg("Remove-despawn enabled.")
        workspace.FallenPartsDestroyHeight = -50000

    elseif a == "off" then
        sysmsg("Remove-despawn disabled.")
        workspace.FallenPartsDestroyHeight = -500
    else
        syserror("Invalid arguments. Please provide arguments 'off' or 'on'.")
    end
end

CMDS.crystaldef = function(sender,args)
    if MemberP == true then
        local element = "Crystal"
		local spell = "Crystalline Annihilation"
		Remotes.DoClientMagic:FireServer(element,spell)
		Remotes.DoMagic:InvokeServer(element,spell,nil)
		task.wait(0.25)
		for i=1,100 do
			Remotes.ClientData:FireServer()
			task.wait()
		end
    end
end

CMDS.checkrank = function(sender,args)
   if MemberP then
      if args[1] then
         local Target = GetPlayer(args[1])
         if table.find(Database['Ranks'][Target.Name],Target.Name) then
         local TargetRank = tostring(Database['Ranks']['Target.Name'][3])
         print(Target.Name.." Returned as a "..TargetRank..".")
         
       else
         print("Player is not a quill user")
       end
      end
   end
end


CMDS.aciddef = function(sender,args)
    if MemberP == true then
        local ohString1 = "Aurora"
        local ohString2 = "Polaris"
        
        game:GetService("ReplicatedStorage").Remotes.DoClientMagic:FireServer(ohString1, ohString2)
        
        local ohString1 = "Acid"
        local ohString2 = "Toxic Basilisk"

        local ohTable3 = {
            ["Floor"] = CFrame.new(-1999999999, -1999999999, -1999999999, -0, 1, -0, -0, 0, -1, -1, 0, -0),
            ["Direction"] = CFrame.new(-1999999999, -1999999999, -1999999999, -0.723236084, -0, -0.690600812, -0, 0.999999881, -0, 0.690600872, 0, -0.723236024),
            ["Ignore"] = true
        }

        game:GetService("ReplicatedStorage").Remotes.DoMagic:InvokeServer(ohString1, ohString2, ohTable3)

        game.Players.LocalPlayer.Character.Head:WaitForChild("BodyPosition"):Destroy()
    end
end

CMDS.antitech = function(sender,args)
    if MemberP == true then
        sysmsg("Deleting all Tech Disks.")
        for i,v in pairs(game:GetService("Workspace")[".Ignore"][".LocalEffects"]:GetChildren()) do
            if v.Name == "LightDisc" then
                v:Destroy()
            end
        end
        for i,v in pairs(game:GetService("Players").LocalPlayer.PlayerScripts:GetChildren()) do
            if v.Name == "DiscScript" then
                v:Destroy()
            end
        end
    end
end

CMDS.clearmap = function(sender,args)
    if VIPP == true then
        local ohString1 = "LightDisc1"
        game:GetService("ReplicatedStorage").Remotes.RemoteEvent:FireServer(ohString1)
        local ohString1 = "LightDiscDelete"
        game:GetService("ReplicatedStorage").Remotes.RemoteEvent:FireServer(ohString1)
        local ohString1 = "Technology"
        local ohString2 = "Light Disc"
        game:GetService("ReplicatedStorage").Remotes.DoClientMagic:FireServer(ohString1, ohString2)
        local ohTable3 = {
            ["Position"] = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
        }
        game:GetService("ReplicatedStorage").Remotes.DoMagic:InvokeServer(ohString1, ohString2, ohTable3)
    end
end

CMDS.infyield = function(sender,args)
    if MemberP == true then
        loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
    end
end

CMDS.infstamina = function(sender,args)
    if VIPP == true then
        if string.lower(args[1]) == "on" then
            sysmsg("InfStamina enabled")
            getgenv().InfStamina = true
        elseif string.lower(args[1]) == "off" then
            sysmsg("InfStamina disabled.")
            getgenv().InfStamina = false
        else
            syserror("Invalid arguments, please provide either 'on' or 'off'.")
        end
    end
end

CMDS.prefix = function(sender,args)
    local stringl = string.lower(args[1])
    if 1 < #stringl then
        syserror("Too many characters. Please keep prefixes limited to #1 character.")
    elseif #stringl == 1 then
        Prefix = stringl
        writefile("prefix.txt",Prefix)
        sysmsg("Your prefix has been changed to "..tostring(Prefix)..".")
    elseif args[1] == nil then
        syserror("Invalid arguments, please provide 1 CHARACTER for your prefix.")
    end
end

CMDS.trackmouse = function(sender,args)
    if VIPP == true then
        local target = GetPlayer(args[1])
        local loweredargs = string.lower(tostring(args[2]))
        if loweredargs == "on" then
            local targetfolder = workspace:FindFirstChild("folder"..target.Name)
            targetfolder.Part.Transparency = 0
            sysmsg("Tracking "..target.Name.."'s mouse.")
        elseif loweredargs == "off" then
            local targetfolder = workspace:FindFirstChild("folder"..target.Name)
            targetfolder.Part.Transparency = 1
            sysmsg("Stopped tracking "..target.Name.."'s mouse.")
        else
            syserror("Invalid arguments, please provide ARG[1] 'player name' ARG[2] 'on' or 'off'.")
        end
    end
end

CMDS.commands = function(sender,args)
    sysmsg(allcommands)
    rconsoleprint(allcommands)
end

coroutine.wrap(function()
    while task.wait(.1) do
        if getgenv().InfStamina == true then
            local ohString1 = "Running"
            local ohBoolean2 = false

            game:GetService("ReplicatedStorage").Remotes.PlayerData:FireServer(ohString1, ohBoolean2)
            
        end
    end
end)()


print("Successful load")
